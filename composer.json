{
    "name": "helhum/dotenv-connector",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "Makes it possible to set environment variables for composer projects.",
    "keywords": [
        "composer", "env", "12factor", "dotenv"
    ],
    "homepage": "http://github.com/helhum/dotenv-connector",
    "authors": [
        {
            "name": "Helmut Hummel",
            "email": "info@helhum.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Helhum\\DotEnvConnector\\": "src/"
        }
    },
    "require": {
        "php": ">=5.3.7",
        "composer-plugin-api": "^1.0",
        "vlucas/phpdotenv": "^2.0.1"
    },
    "require-dev": {
        "composer/composer": "^1.0.0",
        "phpunit/phpunit": "~4.8.0",
        "mikey179/vfsStream": "~1.6.0"
    },
    "scripts": {
        "pre-autoload-dump": "echo '<?php' > res/PHP/dotenv-include.php"
    },
    "extra": {
        "class": "Helhum\\DotEnvConnector\\Plugin",
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
